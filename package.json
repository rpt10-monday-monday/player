{
  "name": "player",
  "version": "1.0.0",
  "description": "Player service for Spotify clone application",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mistergravy/player.git"
  },
  "keywords": [
    "Player",
    "Audio"
  ],
  "author": "Jared Horne",
  "license": "ISC",
  "engines": {
    "node": ">=6.13.0"
  },
  "bugs": {
    "url": "https://github.com/mistergravy/player/issues"
  },
  "jestWebpackResolver": {
    "silent": true,
    "webpackConfig": "./webpack.config.js"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./tests/setup/setupEnzyme.js",
    "testPathIgnorePatterns": [
      "./tests/setup/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "client",
      ".",
      "node_modules",
      "bower_components",
      "shared"
    ],
    "modulePaths": [
      "/shared/vendor/modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "resolver": "jest-webpack-resolver"
  },
  "homepage": "https://github.com/mistergravy/player#readme",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "webpack --watch",
    "start": "nodemon ./server/app.js",
    "db:setup": "node mongoDB/seed.js",
    "sqs_poll": "node ./sqs-queue/sqs_receivemessage.js"
  },
  "devDependencies": {
    "babel-jest": "^23.6.0",
    "chai": "^4.2.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "file-loader": "^3.0.1",
    "jest": "^22.4.4",
    "jest-styled-components": "^6.3.1",
    "jest-webpack-resolver": "^0.3.0",
    "mocha": "^5.2.0",
    "react-test-renderer": "^16.7.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "async": "^2.6.1",
    "axios": "^0.18.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "express": "^4.16.2",
    "jest": "^22.3.0",
    "mongoose": "^5.4.1",
    "morgan": "^1.9.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-audio-player": "^0.11.0",
    "react-dom": "^16.7.0",
    "react-h5-audio-player": "^1.1.0",
    "react-player": "^1.8.0",
    "react-responsive-audio-player": "^1.5.0",
    "sqs-consumer": "^4.0.0",
    "styled-components": "^4.1.3",
    "webpack": "^3.11.0"
  }
}
